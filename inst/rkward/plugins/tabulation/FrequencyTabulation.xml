<!DOCTYPE rkplugin>
<document>
	<code file="FrequencyTabulation.js" />
	<help file="FrequencyTabulation.rkh" />

	<dialog label="Distribución de frecuencias">
		<tabbook id="tabs">
			<tab label="Variables" id="tab_vars">
				<row id="row_vars">
					<varselector id="vars" />
					<varslot id="var1" label="Variable a tabular" source="vars" required="true" />
				</row>
			</tab>
		</tabbook>
	</dialog>
	<wizard label="Distribución de frecuencias">
		<page id="pag_vars">
			<text id="txt_var">
				Seleccionar la variable a tabular. 
			</text>
			<copy id="row_vars" />
		</page>
	</wizard>
</document>

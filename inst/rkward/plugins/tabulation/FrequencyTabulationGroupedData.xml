<!DOCTYPE rkplugin>
<document>
	<code file="FrequencyTabulationGroupedData.js" />
	<help file="FrequencyTabulationGroupedData.rkh" />

	<logic>
		<connect client="cells.variable" governor="variable.available" />
	</logic>

	<dialog label="Distribución de frecuencias">
		<tabbook>
			<tab label="Variables" id="tab_vars">
				<row id="row_vars">
					<varselector id="selector" label="Seleccionar variable"/>
					<varslot id="variable" label="Variable a tabular" source="selector" types="number" required="true" />
				</row>
			</tab>
			<tab label="Clases" id="tab_cells">
				<embed id="cells" component="rkTeaching::cells"/>
			</tab>
		</tabbook>
	</dialog>
	
	<wizard label="Distribución de frecuencias">
		<page id="pag_vars">
			<text id="txt_var">
				Selecionar la variable a tabular.
			</text>
			<copy id="row_vars" />
		</page>
		<embed id="cells" component="rkTeaching::cells"/>
	</wizard>
</document>

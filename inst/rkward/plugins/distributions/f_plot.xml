<!DOCTYPE rkplugin>
<document>
	<code file="f_plot.js" />
	<help file="f_plot.rkh" />
	<logic>
		<insert snippet="commonLogic"/>
	</logic>
	
	<snippets>
		<snippet id="rangeOptions">
			<frame id="range" label="Rango" checkable="true" checked="false">
				<spinbox default_precision="2" type="real" id="min" initial="0" label="Mínimo" />
				<spinbox default_precision="2" type="real" id="max" initial="10" label="Máximo" />
			</frame>
		</snippet>
		<snippet id="distOptions">
			<frame id="parameters" label="Parameters">
				<spinbox default_precision="2" type="integer" id="df1" initial="1" label="Degrees of freedom del numerador" />
				<spinbox default_precision="2" type="integer" id="df2" min="0.01" initial="1" label="Degrees of freedom del denominador"/>
			</frame>
		</snippet>
		<snippet id="parametersWizard">
			<text>
				Introducir los grados de libertad del numerador y del denominador de la distribución F de Fisher. 	
			</text>
		</snippet>
		<include file="plot_dist_common_snippets.xml"/>
	</snippets>
	
	<dialog label="Gráfico de probabilidad F de Fisher" >
		<insert snippet="continousLayout"/>
	</dialog>

	<wizard label="Gráfico de probabilidad F de Fisher">
		<insert snippet="continousWizard"/>
	</wizard>
</document>
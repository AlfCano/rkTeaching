<!DOCTYPE rkplugin>
<document>
	<code file="poisson_quantiles.js" />
	<snippets>
		<snippet id="dist_options">
			<frame id="parameters" label="Parámetros">
				<spinbox id="lambda" label="Media" default_precision="2" type="real" min="0" initial="1" />
			</frame>
		</snippet>
		<snippet id="parameters_wizard">
			<text>
				Introducir la media de la distribución de Poisson. 	
			</text>
		</snippet>
		<include file="dist_snippets.xml"/>
	</snippets>
	<dialog label="Cuantiles Poisson" >
		<insert snippet="quantiles_layout"/>
	</dialog>

	<wizard label="Cuantiles Poisson" >
		<insert snippet="quantiles_wizard"/>
	</wizard>	
</document>
		<component type="standard" id="levene_test" file="levene_test.xml" label="Levene's test" />		
		<component type="standard" id="mean_sample_size" file="mean_sample_size.xml" label="Sample size to estimate one mean" />
		<component type="standard" id="test_t_sample_size" file="test_t_sample_size.xml" label="Sample size for the t-test" />
		<component type="standard" id="proportion_sample_size" file="proportion_sample_size.xml" label="Sample size to estimate one proportion" />

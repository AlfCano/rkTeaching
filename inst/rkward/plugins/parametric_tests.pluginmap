<!DOCTYPE rkpluginmap>
<document base_prefix="parametric_tests/" namespace="rkTeaching">
	<components>
		<component type="standard" id="one_sample_t_test" file="one_sample_t_test.xml" label="t-test for the mean of one population" />
		<component type="standard" id="two_independent_samples_t_test" file="two_independent_samples_t_test.xml" label="t-test for comparing the means of two independent populations" />
		<component type="standard" id="two_paired_samples_t_test" file="two_paired_samples_t_test.xml" label="t-test for comparing the means of two paired populations" />
		<component type="standard" id="anova" file="anova.xml" label="ANOVA" />
		<component type="standard" id="one_proportion_test" file="one_proportion_test.xml" label="Test for one proportion" />
		<component type="standard" id="two_proportions_test" file="two_proportions_test.xml" label="Test for comparing two proportions" />

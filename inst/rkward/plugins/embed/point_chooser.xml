<!DOCTYPE rkplugin>
<document>
	<code file="point_chooser.js" />
	<help file="point_chooser.rkh" />
	<logic></logic>
	<dialog label="Point type">
		<dropdown id="point" label="Point symbol">
			<option value="" label="default" checked="true" />
			<option value="0" label="Non-filled square" />
			<option value="1" label="Non-filled circle" />
			<option value="2" label="Non-filled triangle" />
			<option value="5" label="Non-filled diamond" />
			<option value="15" label="Filled square" />
			<option value="16" label="Filled circle" />
			<option value="17" label="Filled triangle" />
			<option value="18" label="Filled diamond" />
		</dropdown>
	</dialog>
	<wizard label="Point type">
		<text>
			<p>Choose the point symbol.</p>
		</text>
		<copy id="point" />
	</wizard>
</document>
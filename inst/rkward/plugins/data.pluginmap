<!DOCTYPE rkpluginmap>
<document base_prefix="data/" namespace="rkTeaching" id="datapluginmap">
	<components>
		<component type="standard" id="filter" file="filter.xml" label="Filtrar datos" />
		<component type="standard" id="variable_calculation" file="variable_calculation.xml" label="Calcular variable" />
		<component type="standard" id="recode" file="recode.xml" label="Recodificar variable" />
		<component type="standard" id="weight" file="weight.xml" label="Ponderar datos" />
